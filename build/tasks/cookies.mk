
CHOOSE_TARGET_PACKAGE := $(PRODUCT_OUT)/choose-$(ROM_VERSION).zip

.PHONY: cookies
cookies: $(INTERNAL_OTA_PACKAGE_TARGET)
	$(hide) ln -f $(INTERNAL_OTA_PACKAGE_TARGET) $(CHOOSE_TARGET_PACKAGE)
	$(hide) $(MD5SUM) $(CHOOSE_TARGET_PACKAGE) | sed "s|$(PRODUCT_OUT)/||" > $(CHOOSE_TARGET_PACKAGE).md5sum
	@echo -e "Package complete: $(CHOOSE_TARGET_PACKAGE)" >&2
